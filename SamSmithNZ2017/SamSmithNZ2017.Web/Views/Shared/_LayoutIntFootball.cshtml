<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>World Cup Analytics</title>
    <link rel="shortcut icon" sizes="32x32" href="~/Content/IntFootball/favicon.ico">
    @Styles.Render("~/Content/globalCSS")
    @Scripts.Render("~/bundles/globalJS")
    @Scripts.Render("~/bundles/intFootballServicesJS")
    @Scripts.Render("~/bundles/intFootballControllersJS")
