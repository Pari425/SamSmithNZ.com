<body ng-app="IntFootballApp" @*ng-controller="layoutController"*@>
    <div class="headerandlogo">
        <table class="PercentWidth100">
            <tr>
                <td class="PercentWidth100">
                    <a href="@Url.Action("Index", "IntFootball")" class="headerText">
                        <img src="@Url.Content("~/Content/IntFootball/WorldCup.png")" alt="" class="headerImg" />
                        <span class="headerText">World Cup Analytics</span>
                    </a>
                </td>
            </tr>
        </table>
    </div>
    <div class="contentFrame">
        <div class="BreadCrumbs" style="padding-left: 1.5em;">
            &nbsp;&nbsp;&nbsp;  @Html.ActionLink("Home", "Index", "IntFootball") > <span id="lblBreadCrumbLocation">loading...</span>
        </div>
        <div style="padding-left: 1.5em;">
            <h2>@ViewBag.Title</h2>
        </div>
        <div class="contentMain">
            @RenderBody()
        </div>
    </div>

    <div class="footer">
        <div class="tableofcontents" style="width: 100%; text-align: center;">
            Copyright &copy;2000 - @DateTime.Now.Year SamSmithNZ.com
        </div>
    </div>

</body>
</html>
